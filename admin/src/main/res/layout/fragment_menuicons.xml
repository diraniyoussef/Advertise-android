<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">
        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:columnCount="4"
            android:padding="20dp">
            <ImageButton
                android:id="@+id/imagebutton_ic_remove_red_eye_black_24dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_remove_red_eye_black_24dp"
                app:srcCompat="@drawable/ic_remove_red_eye_black_24dp" />
            <ImageButton
                android:id="@+id/imagebutton_ic_weekend_black_24dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_weekend_black_24dp"
                app:srcCompat="@drawable/ic_weekend_black_24dp" />
            <ImageButton
                android:id="@+id/imagebutton_ic_action_home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_action_home"
                app:srcCompat="@drawable/ic_action_home" />
            <ImageButton
                android:id="@+id/imagebutton_ic_home_black_24dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_home_black_24dp"
                android:backgroundTint="@color/colorBlack"
                android:foregroundGravity="center|clip_horizontal"
                app:srcCompat="@drawable/ic_home_black_24dp" />
            <ImageButton
                android:id="@+id/imagebutton_ic_menu_camera"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_menu_camera"
                app:srcCompat="@drawable/ic_menu_camera" />
            <ImageButton
                android:id="@+id/imagebutton_ic_menu_gallery"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_menu_gallery"
                app:srcCompat="@drawable/ic_menu_gallery" />
            <ImageButton
                android:id="@+id/imagebutton_ic_menu_send"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_menu_send"
                app:srcCompat="@drawable/ic_menu_send" />
            <ImageButton
                android:id="@+id/imagebutton_ic_menu_share"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_menu_share"
                app:srcCompat="@drawable/ic_menu_share" />
            <ImageButton
                android:id="@+id/imagebutton_ic_menu_slideshow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_menu_slideshow"
                app:srcCompat="@drawable/ic_menu_slideshow" />
            <ImageButton
                android:id="@+id/imagebutton_ic_perm_camera_mic_black_24dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_perm_camera_mic_black_24dp"
                app:srcCompat="@drawable/ic_perm_camera_mic_black_24dp" />
            <ImageButton
                android:id="@+id/imagebutton_ic_person_black_24dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_person_black_24dp"
                app:srcCompat="@drawable/ic_person_black_24dp" />
            <ImageButton
                android:id="@+id/imagebutton_ic_menu_manage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_menu_manage"
                app:srcCompat="@drawable/ic_menu_manage" />
            <ImageButton
                android:id="@+id/imagebutton_ic_person_pin_black_24dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_person_pin_black_24dp"
                app:srcCompat="@drawable/ic_person_pin_black_24dp" />
            <ImageButton
                android:id="@+id/imagebutton_ic_pets_black_24dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_pets_black_24dp"
                app:srcCompat="@drawable/ic_pets_black_24dp" />
            <ImageButton
                android:id="@+id/imagebutton_ic_refresh_black_24dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_refresh_black_24dp"
                android:backgroundTint="@color/colorBlack"
                app:srcCompat="@drawable/ic_refresh_black_24dp" />
            <ImageButton
                android:id="@+id/imagebutton_ic_settings_applications_black_24dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_settings_applications_black_24dp"
                app:srcCompat="@drawable/ic_settings_applications_black_24dp" />
            <ImageButton
                android:id="@+id/imagebutton_ic_settings_ethernet_black_24dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_settings_ethernet_black_24dp"
                app:srcCompat="@drawable/ic_settings_ethernet_black_24dp" />
            <ImageButton
                android:id="@+id/imagebutton_ic_shopping_basket_black_24dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_shopping_basket_black_24dp"
                app:srcCompat="@drawable/ic_shopping_basket_black_24dp" />
            <ImageButton
                android:id="@+id/imagebutton_ic_shopping_cart_black_24dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_shopping_cart_black_24dp"
                app:srcCompat="@drawable/ic_shopping_cart_black_24dp" />
            <ImageButton
                android:id="@+id/imagebutton_ic_touch_app_black_24dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_touch_app_black_24dp"
                app:srcCompat="@drawable/ic_touch_app_black_24dp" />
            <ImageButton
                android:id="@+id/imagebutton_ic_vignette_black_24dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_vignette_black_24dp"
                app:srcCompat="@drawable/ic_vignette_black_24dp" />
            <ImageButton
                android:id="@+id/imagebutton_ic_no_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:tag="ic_no_icon"
                app:srcCompat="@drawable/ic_no_icon" />
        </GridLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>